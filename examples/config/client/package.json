{
  "name": "m2m-application",
  "version": "1.0.0",
  "private": true,
  "description": "node-m2m application.",
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "m2m": "^1.2.6"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon client.js --delay 2000ms"
  },
  "nodemonConfig": {
    "verbose": true,
    "restartable": "rs",
    "ignore": [
      ".git",
      "public"
    ],
    "ignoreRoot": [
      ".git",
      "public"
    ],
    "execMap": {
      "js": "node"
    },
    "watch": [
      "node_modules/m2m/mon"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
  }
}
